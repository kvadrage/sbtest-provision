global:
  save_local: "disable"
  ignore_ifaces: ["eth0", "vagrant"]

  services:
    clock:
      # List of time zones: https://en.wikipedia.org/wiki/List_of_tz_database_time_zones
      timezone: "Europe/Moscow"
      ntp:
        servers: ["10.35.81.200"]
    zabbix:
      server: "10.35.81.200"
    aaa:
      auth: "default" # "default", "radius", "tacacs"
      radius:
        server: "10.35.81.200"
        secret: "Mellanox"
      tacacs:
        server: "10.35.81.200"
        secret: "Mellanox"


